  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davecarter/npm-project/issues"
  },
  "homepage": "https://github.com/davecarter/npm-project#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.7.0",
    "opener": "^1.4.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
